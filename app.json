{
  "expo": {
    "name": "akari-app",
    "slug": "akari-new",
    "version": "1.0.0",
    "extra": {
      "supportsRTL": true,
      "forcesRTL": true,
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "d46e25b9-14a6-4e92-982b-b81c47a21381"
      }
    },
    "scheme": "akari-app",
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-dev-launcher",
        {
          "launchMode": "most-recent"
        }
      ],
      "expo-font",
      "expo-secure-store",
      "expo-build-properties",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#ffffff",
          "image": "./assets/splash_1.png"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "tsconfigPaths": true
    },
    "orientation": "portrait",
    "icon": "./assets/splash_1.png",
    "userInterfaceStyle": "light",
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/splash_1.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.versetech.akarinew"
    },
    "eas": {
      "projectId": "ba1a0d5b-7ed5-4a78-9d47-8c664c514673"
    }
  }
}
